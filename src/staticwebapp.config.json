{
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/72f988bf-86f1-41af-91ab-2d7cd011db47/v2.0",
          "clientIdSettingName": "AAD_CLIENT_ID",
          "clientSecretSettingName": "AAD_CLIENT_SECRET"
        }
      }
    }
  },
  "routes": [
    {
      "route": "/login",
      "methods": null,
      "allowedRoles": [],
      "statusCode": null,
      "redirect": "/.auth/login/aad",
      "rewrite": null
    },
    {
      "route": "/logout",
      "methods": null,
      "allowedRoles": [],
      "statusCode": null,
      "redirect": "/.auth/logout",
      "rewrite": null
    },
    {
      "route": "/.auth/login/aad/callback",
      "methods": null,
      "allowedRoles": [],
      "statusCode": null,
      "redirect": "/",
      "rewrite": null
    },
    {
      "route": "/*",
      "methods": null,
      "allowedRoles": [
        "authenticated"
      ],
      "statusCode": null,
      "redirect": null,
      "rewrite": null
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": null
  },
  "responseOverrides": {
    "401": {
      "redirect": "/login",
      "statusCode": 302
    },
    "UnauthorizedPage": {
      "redirect": "/login",
      "rewrite": null,
      "statusCode": 302
    },
    "UnauthorizedPageNoUser": {
      "redirect": "/login",
      "rewrite": null,
      "statusCode": 302
    }
  }
}